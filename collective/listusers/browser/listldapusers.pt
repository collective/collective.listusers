<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      i18n:domain="collective.listusers">
  <body>
    <div id="contacts">
      <ul id="user-listing"
          tal:define="users python: view.options['users']"
          tal:repeat="user users"> 
        <li tal:define="fullname python: user[1]['fullname']" tal:content="fullname"></li>
      </ul>
      <input type="hidden" tal:attributes="value view/options/cookie" />
    </div>
  </body>
</html>
